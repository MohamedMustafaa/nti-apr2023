{{>header}}
{{>nav}}
<section class="my-5 container">
  {{#if task}}
  <div>
    <h3>{{task.id}}</h3>
    <h3>{{task.title}}</h3>
    <h3>{{task.content}}</h3>
    <h3>{{task.status}}</h3>
    <h3>{{task.dueDate}}</h3>
  </div>
  {{else}}
  <div class="alert alert-danger">
    No task found
  </div>
  {{/if}}
</section>
{{>footer}}